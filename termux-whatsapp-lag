download(){
	test ! -d ~/storage && \
		termux-setup-storage
	printf "[NOTICE] Termux and Termux:Api must be signed with same keystore\n"
	printf "Choose number\n"
	printf "Where you installing Termux:\n"
	printf "1) Play Store\n"
	printf "2) F-Droid\n"
	read answer

	[[ ! "$answer" =~ ^[0-9]$ ]] || \
		[ $answer -lt 1 ] || \
		[ $answer -gt 2 ] && \
		printf "Invalid number\n" && exit 1;




	case "$answer" in
		1) DOWNLOAD_URL="https://play.google.com/store/apps/details/?id=${PACKAGENAME}"
	
			# Installing app from playstore
			am start \
				-a "android.intent.action.VIEW" \
				-d $DOWNLOAD_URL > /dev/null
			;;
		2) DOWNLOAD_URL="https://f-droid.org/repo/com.termux.api_25.apk"
			
			FILENAME="${EXTERNAL_STORAGE}/Download/$(basename $DOWNLOAD_URL)"

			if test ! -r $FILENAME; then
				am start \
				-a android.intent.action.VIEW \
				-d $DOWNLOAD_URL > /dev/null
			fi
			test $? -eq 1 && exit 1
			local text="Please wait"
			while [ ! -r "${FILENAME}" ]; do
				printf "$text"
				sleep 20
				text="."
			done
			am start \
				-a android.intent.action.INSTALL_PACKAGE \
				-d file://${FILENAME} > /dev/null
			;;
	esac


}


if test -z "$(which termux-clipboard-set)"; then
	apt install -y termux-api
fi
IS_INSTALLED=$(pm list packages | grep "${PACKAGENAME}")

if test -z "$IS_INSTALLED"; then
	printf "You must confirm to download Termux:Api for run this script\n"
	printf "Please wait\n"
	sleep 5

	download

	printf "Run again.. if you has installing dependencies"

else 
	printf "$DOC"
	echo "$UNICODE" | termux-clipboard-set
fi
